<Window x:Class="CreatePiles.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CreatePiles"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="450">
    <Grid>
        <Button Content="Create IFC file" HorizontalAlignment="Center" Height="32" VerticalAlignment="Top" Width="262" Click="Button_Click" Margin="0,131,0,0"/>
        <TextBox x:Name="inputCsvFile" HorizontalAlignment="Center" Margin="0,39,0,0" TextWrapping="Wrap" Text="C:\temp\Piles.csv" VerticalAlignment="Top" Width="262"/>
        <Label Content="Input csv file:" HorizontalAlignment="Center" Margin="0,8,0,0" VerticalAlignment="Top" Width="262"/>
        <TextBox x:Name="outputFolder" HorizontalAlignment="Center" TextWrapping="Wrap" Text="C:\temp\Piles.ifc" VerticalAlignment="Top" Width="262" Margin="0,99,0,0"/>
        <Label Content="Output folder:" HorizontalAlignment="Center" Margin="0,68,0,0" VerticalAlignment="Top" Width="262"/>
        
    </Grid>
</Window>
